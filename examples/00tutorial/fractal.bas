10 REM "Fractal from RUNCPM, Copyright unknown"
20 REM "Suggested by Guido, ported by Stefan"
100 ST=MILLIS(1)
200 REM "Main loop"
210 FOR Y=-12 TO 12
220 FOR X=-49 TO 32
230 CA=X*0.0458
240 CB=Y*0.08333
250 A=CA
260 B=CB
270 FOR I=0 TO 15
280 T=A*A-B*B+CA
290 B=2*A*B+CB
300 A=T
310 IF(A*A+B*B)>4 THEN 350
320 NEXT I
330 PRINT " ";
340 GOTO 370
350 IF I>9 THEN I=I+7
360 PUT 48+I
370 NEXT X
380 PRINT
390 NEXT Y
400 PRINT "TIME TO RUN:";MILLIS(1)-ST;"MSECS"
